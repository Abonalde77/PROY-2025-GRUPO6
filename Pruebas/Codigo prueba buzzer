from machine import Pin
import time

# Configuración de los pines
buzzer = Pin(15, Pin.OUT)  # Buzzer en el pin GPIO 15
led = Pin(25, Pin.OUT)     # LED integrado en el pin GPIO 25

# Función principal
while True:
    # Encender el buzzer y el LED integrado
    buzzer.on()
    led.on()
    time.sleep(1)  # Esperar 1 segundo

    # Apagar el buzzer y el LED integrado
    buzzer.off()
    led.off()
    time.sleep(1)  # Esperar 1 segundo

